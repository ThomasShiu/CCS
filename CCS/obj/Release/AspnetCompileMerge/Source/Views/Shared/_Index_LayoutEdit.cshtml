<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MAIN</title> 
    <link href="~/Content/easyui-1.5/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Content/easyui/themes/color.css" rel="stylesheet" />
    <link href="~/Content/easyui/themes/icon.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
</head>
<body style="background-color:lightsteelblue">

    @RenderBody()


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/home")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquery-easyui")
    <script src="~/Content/easyui-1.5/jquery.easyui.min.js"></script>
    <script src="~/Content/easyui-1.5/jquery.easyui.plus.js"></script>

    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    @RenderSection("scripts", required: false)

  <script>
      function myformatter(date) {
          var y = date.getFullYear();
          var m = date.getMonth() + 1;
          var d = date.getDate();

          var s = date.getSeconds();
          var mi = date.getMinutes();
          var h = date.getHours();

          return y + '-' + (m < 10 ? ('0' + m) : m) + '-' + (d < 10 ? ('0' + d) : d);
      }

      function myparser(s) {
          if (!s) return new Date();
          var ss = (s.split('-'));
          var y = parseInt(ss[0], 10);
          var m = parseInt(ss[1], 10);
          var d = parseInt(ss[2], 10);
          if (!isNaN(y) && !isNaN(m) && !isNaN(d)) {
              return new Date(y, m - 1, d);
          } else {
              return new Date();
          }
      }

      function myformatter2(date) {
          var y = date.getFullYear();
          var m = date.getMonth() + 1;
          var d = date.getDate();

          var s = date.getSeconds();
          var mi = date.getMinutes();
          var h = date.getHours();

          return y + '-' + (m < 10 ? ('0' + m) : m) + '-' + (d < 10 ? ('0' + d) : d) + ' ' + (h < 10 ? ('0' + h) : h) + ':' + (mi < 10 ? ('0' + mi) : mi);
      }
      function myparser2(s) {
          var t = Date.parse(s);
          if (!isNaN(t)) {
              return new Date(t);
          } else {
              return new Date();
          }
      }
  </script>
</body>
</html>