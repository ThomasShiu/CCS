<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - EIS</title>   
    <link href="~/Content/easyui-1.5/themes/metro/easyui.css" rel="stylesheet" />
    <link href="~/Content/easyui-1.5/themes/color.css" rel="stylesheet" />
    <link href="~/Content/easyui-1.5/themes/icon.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
</head>
<body >
        @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquery-easyui")

    <script src="~/Content/easyui-1.5/jquery.easyui.min.js"></script>
    <script src="~/Content/easyui-1.5/jquery.easyui.plus.js"></script>
    @RenderSection("scripts", required: false)
    @*@RenderPage("~/Views/Shared/_ScriptSection.cshtml")*@

</body>
</html>