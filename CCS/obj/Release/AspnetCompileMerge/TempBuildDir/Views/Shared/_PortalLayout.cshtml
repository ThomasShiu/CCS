<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ERP</title>

    <link href="~/Content/tree/css/tree.css" rel="stylesheet" />
    <link href="~/Content/easyui/themes/black/easyui.css" rel="stylesheet" />
    <link href="~/Content/easyui/themes/color.css" rel="stylesheet" />
    <link href="~/Content/easyui/themes/icon.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)

    @Scripts.Render("~/bundles/jquery")
    <script src="~/Content/jquery.tree.js"></script>
    <script src="~/Content/easyui-1.5/jquery.easyui.min.js"></script>
    <script src="~/Scripts/CCSHome.js"></script>
</head>
<body class="easyui-layout">

    @RenderBody()

    
    
    @*@Scripts.Render("~/bundles/bootstrap")*@

    @RenderSection("scripts", required: false)
    @*@RenderPage("~/Views/Shared/_ScriptSection.cshtml")*@

    <script type="text/javascript">
        $(function () {
            if (Modernizr.canvas)
                $("#result").text("你的瀏覽器支援canvas");
            else
                $("#result").text("你的瀏覽器不支援canvas");
        });
    </script>
</body>
</html>

