<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - EIS</title>   
    <link href="~/Content/easyui-1.5/themes/metro/easyui.css" rel="stylesheet" />
    <link href="~/Content/easyui-1.5/themes/color.css" rel="stylesheet" />
    <link href="~/Content/easyui-1.5/themes/icon.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
</head>
<body >
    <div style="padding:4px 5px 0px 5px;">
        @RenderBody()
        </div>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/jquery-easyui")

        <script src="~/Content/easyui-1.5/jquery.easyui.min.js"></script>
        <script src="~/Content/easyui-1.5/jquery.easyui.plus.js"></script>

        @RenderSection("scripts", required: false)
        @*@RenderPage("~/Views/Shared/_ScriptSection.cshtml")*@
        <script>
            //生成唯一的GUID
            function GetGuid() {
                var s4 = function () {
                    return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1);
                };
                return s4() + s4() + s4() + "-" + s4();
            }
        </script>
</body>
</html>